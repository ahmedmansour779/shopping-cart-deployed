{
  "homepage": "https://ahmedmansour779.github.io/shopping-cart-deployed/",
  "name": "shopping-cart",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
